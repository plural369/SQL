-- predefined type, no DDL - MDSYS.SDO_GEOMETRY

-- predefined type, no DDL - XMLTYPE

CREATE TABLE ALUNO 
    ( 
     ID        NUMBER  NOT NULL , 
     NOME      VARCHAR2 (50) , 
     CODIGO    NUMBER , 
     DATA_NASC DATE 
    ) 
;

ALTER TABLE ALUNO 
    ADD CONSTRAINT ALUNO_PK PRIMARY KEY ( ID ) ;

CREATE TABLE DISCIPLINA 
    ( 
     ID               NUMBER  NOT NULL , 
     NOME             VARCHAR2 (50) , 
     CREDITO          NUMBER , 
     CODIGO           NUMBER , 
     PROFESSOR_ID     NUMBER  NOT NULL , 
     PRE_REQUISITO_ID NUMBER 
    ) 
;

ALTER TABLE DISCIPLINA 
    ADD CONSTRAINT DISCIPLINA_PK PRIMARY KEY ( ID ) ;

CREATE TABLE MATRICULA 
    ( 
     ID            NUMBER  NOT NULL , 
     NOTA          NUMBER (4,2) , 
     SITUACAO      VARCHAR2 (20) , 
     ALUNO_ID      NUMBER  NOT NULL , 
     DISCIPLINA_ID NUMBER  NOT NULL 
    ) 
;

ALTER TABLE MATRICULA 
    ADD CONSTRAINT MATRICULA_PK PRIMARY KEY ( ID ) ;

CREATE TABLE PROFESSOR 
    ( 
     ID     NUMBER  NOT NULL , 
     CODIGO NUMBER , 
     NOME   VARCHAR2 (50) 
    ) 
;

ALTER TABLE PROFESSOR 
    ADD CONSTRAINT PROFESSOR_PK PRIMARY KEY ( ID ) ;

ALTER TABLE DISCIPLINA 
    ADD CONSTRAINT DISCIPLINA_PROFESSOR_FK FOREIGN KEY 
    ( 
     PROFESSOR_ID
    ) 
    REFERENCES PROFESSOR 
    ( 
     ID
    ) 
    ON DELETE CASCADE 
;

ALTER TABLE MATRICULA 
    ADD CONSTRAINT MATRICULA_ALUNO_FK FOREIGN KEY 
    ( 
     ALUNO_ID
    ) 
    REFERENCES ALUNO 
    ( 
     ID
    ) 
    ON DELETE CASCADE 
;

ALTER TABLE MATRICULA 
    ADD CONSTRAINT MATRICULA_DISCIPLINA_FK FOREIGN KEY 
    ( 
     DISCIPLINA_ID
    ) 
    REFERENCES DISCIPLINA 
    ( 
     ID
    ) 
    ON DELETE CASCADE 
;

CREATE INDEX PRE_REQUISITO_ID_INDEX
ON DISCIPLINA(PRE_REQUISITO_ID);

CREATE SEQUENCE ALN_ID_SEQ 
START WITH 1 
    NOCACHE 
    ORDER ;

CREATE OR REPLACE TRIGGER ALN_ID_TRG 
BEFORE INSERT ON ALUNO 
FOR EACH ROW 
WHEN (NEW.ID IS NULL) 
BEGIN 
    :NEW.ID := ALN_ID_SEQ.NEXTVAL; 
END;
/

